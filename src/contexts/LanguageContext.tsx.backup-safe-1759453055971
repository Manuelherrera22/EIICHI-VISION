// Provider del contexto de idioma
export function LanguageProvider({ children }: { children: React.ReactNode }) {
  const [language, setLanguage] = useState<Language>('en'); // Siempre empezar con 'en' para evitar hidrataci√≥n incorrecta
  const [isHydrated, setIsHydrated] = useState(false);

  // Funci√≥n para obtener traducci√≥n
  const t = (key: string, variables?: Record<string, string>): string => {
    let translation: string = translations[language][key as keyof typeof translations[typeof language]] || key;
    
    // Interpolaci√≥n de variables
    if (variables) {
      Object.entries(variables).forEach(([varKey, varValue]) => {
        translation = translation.replace(new RegExp(`{${varKey}}`, 'g'), String(varValue));
      });
    }
    
    return translation;
  };

  // Funci√≥n para cambiar idioma con persistencia
  const handleSetLanguage = (newLanguage: Language) => {
    setLanguage(newLanguage);
    if (typeof window !== 'undefined') {
      localStorage.setItem('language', newLanguage);
    }
  };

  // Detectar idioma del navegador al cargar (solo en el cliente)
  useEffect(() => {
    setIsHydrated(true);
    
    // Intentar obtener el idioma guardado del localStorage
    const savedLanguage = localStorage.getItem('language') as Language;
    if (savedLanguage && ['en', 'ja', 'ar', 'es'].includes(savedLanguage)) {
      setLanguage(savedLanguage);
      return;
    }
    
    // Si no hay idioma guardado, detectar idioma del navegador
    const browserLang = navigator.language.split('-')[0];
    if (browserLang === 'ja' || browserLang === 'ar') {
      handleSetLanguage(browserLang as Language);
    }
  }, []);

  const value = {
    language,
    setLanguage: handleSetLanguage,
    t,
    isHydrated,
  };

  return (
    <LanguageContext.Provider value={value}>
      {children}
    </LanguageContext.Provider>
  );
}
