{
  "name": "Tabiji House - Japanese Real Estate Intelligence",
  "short_name": "Tabiji House",
  "description": "Plataforma inteligente para inversiones inmobiliarias en Japón con análisis avanzado de IA",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#3b82f6",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "es",
  "dir": "ltr",
  "categories": ["business", "finance", "lifestyle"],
  "icons": [
    {
      "src": "/tabijihouse-removebg-preview.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/tabijihouse-removebg-preview.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/tabijihouse-removebg-preview.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "/tabijihouse-removebg-preview.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "/tabijihouse-removebg-preview.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "/tabijihouse-removebg-preview.png",
      "sizes": "48x48",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshot-desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard principal en escritorio"
    },
    {
      "src": "/screenshot-mobile.png",
      "sizes": "375x667",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Dashboard optimizado para móvil"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "Acceso rápido al dashboard principal",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/tabijihouse-removebg-preview.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Proyectos",
      "short_name": "Proyectos",
      "description": "Ver proyectos disponibles",
      "url": "/projects",
      "icons": [
        {
          "src": "/tabijihouse-removebg-preview.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Búsqueda Global",
      "short_name": "Buscar",
      "description": "Búsqueda rápida en toda la plataforma",
      "url": "/dashboard?search=true",
      "icons": [
        {
          "src": "/tabijihouse-removebg-preview.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://tabijihouse.com/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "capture_links": "new-client",
  "file_handlers": [
    {
      "action": "/dashboard/import",
      "accept": {
        "application/pdf": [".pdf"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "text/csv": [".csv"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+tabiji",
      "url": "/dashboard?action=%s"
    }
  ],
  "share_target": {
    "action": "/dashboard/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "application/pdf", "text/plain"]
        }
      ]
    }
  }
}
