# Google Maps API Key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Netlify Environment Variables
NEXT_PUBLIC_SITE_URL=https://your-site-name.netlify.app
NEXT_PUBLIC_NETLIFY_DEPLOY_CONTEXT=production

# Analytics (opcional)
NEXT_PUBLIC_GA_ID=your_google_analytics_id_here

# Contact Form (opcional - para Netlify Forms)
NEXT_PUBLIC_NETLIFY_FORM_ID=your_netlify_form_id_here

# Build Configuration
NODE_ENV=production
NEXT_PUBLIC_BUILD_TIME=2025-01-24

# Three.js Configuration
NEXT_PUBLIC_THREE_JS_ENABLED=true
NEXT_PUBLIC_MODEL_PATH=/models/custom/japanese-house.glb
